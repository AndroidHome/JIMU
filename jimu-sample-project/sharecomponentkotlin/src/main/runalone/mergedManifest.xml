<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luojilab.share.kotlin">

    <application xmlns:tools="http://schemas.android.com/tools"
        android:name="com.luojilab.share.runalone.application.KotlinApplication"
        android:allowBackup="true"
        android:label="sharekotlin"
        android:supportsRtl="true"
        android:theme="@style/commonAppTheme"
        tools:replace="android:label">
        <activity android:name=".ShareMessageActivity" />
        <activity android:name=".JavaTarget" />
        <activity android:name="com.luojilab.share.runalone.application.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
